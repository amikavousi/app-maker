{
    "name": "amikavousi/app-maker",
    "description": "AppMaker, designed to help modularize your Laravel project. With AppMaker, you can easily organize your code and improve your project's scalability.",
    "keywords": ["laravel modular", "modular", "app maker", "modularize"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2.0",
        "illuminate/console": "^11.0",
        "illuminate/filesystem": "^11.0",
        "illuminate/support": "^11.0",
        "illuminate/validation": "^11.0",
        "symfony/console": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "AmiKavousi\\AppMaker\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AmiKavousi\\AppMaker\\AppMakerProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Amin Kavousi",
            "email": "amikavousi@gmail.com"
        }
    ],
    "minimum-stability": "dev"
}
